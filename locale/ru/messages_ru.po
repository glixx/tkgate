msgctxt "err.yy.pinformat"
msgid "Illegal pin name format '%s'."
msgstr "Недопустимый формат имени вывода '%s'."

#: en/messages:1745
msgctxt "err.yy.pinname"
msgid "Pin name '%s' not defined for component type '%s'"
msgstr "Имя вывода '%s' не определено для компонента '%s'"

#: en/messages:1746
msgctxt "err.yy.nonetpos"
