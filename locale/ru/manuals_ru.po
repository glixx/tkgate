msgid ""
"<b>Simulator Script Example:</b>\n"
"<font face=courier>\n"
"initial\n"
"  begin\n"
"    $display(\"%t: Start of script\",$time);\n"
"    $tkg$probe(ck, reg_out, clr);\n"
"    $display(\"%t: Set probes\",$time);\n"
"    clr = 1'b0;\n"
"    #200;\n"
"    clr = 1'b1;\n"
"    $display(\"%t: Done with reset\",$time);\n"
"    #500;\n"
"    $stop();\n"
"  end\n"
"</font>"
msgstr ""
"<b>Пример скрипта симуляции:</b>\n"
"<font face=courier>\n"
"initial\n"
"  begin\n"
"    $display(\"%t: Запуск скрипта\",$time);\n"
"    $tkg$probe(ck, reg_out, clr);\n"
"    $display(\"%t: Установка зондов\",$time);\n"
"    clr = 1'b0;\n"
"    #200;\n"
"    clr = 1'b1;\n"
"    $display(\"%t: Завершено со сбросом\",$time);\n"
"    #500;\n"
"    $stop();\n"
"  end\n"
"</font>"

