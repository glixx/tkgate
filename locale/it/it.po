msgstr "Identificatore non ammesso '%s'."

# err.bkpt.badexp
#: it/messages:482
msgid ""
"Syntax error in breakpoint expression. Must be one of: 'net', '!net', "
"'net==value', 'net!=value'"
msgstr ""
"Errore di sintassi nell'espressione di breakpoint. Devi scegliere fra: "
"'net', '!net', 'net==value', 'net!=value'"

# err.bkpt.badnet
#: it/messages:483
msgid ""
"Invalid net name '%s'. Names must start with a letter and contain only "
"letters, digits and '.'."
msgstr ""
"Errore nel nome del collegamento '%s'. I nomi devono iniziare con una "
"lettera e contengono solo lettere, numeri e '.'."

# err.bkpt.badval
#: it/messages:484
msgid "Syntax error in value '%s'. Must be decimal or verilog-style constant."
msgstr ""
"Errore di sintassi nel valore '%s'. Deve essere un decimale o una costante "
"in stile verilog."

# err.bkpt.toomany
#: it/messages:485
msgid "Too many breakpoints. Maximum is %d."
msgstr "Troppi breakpoints. Al massimo %d."

# err.sim.badtmp
#: it/messages:487
msgid "Could not save temporary file '%s' for simulator (disc full?)"
msgstr ""
"Non ho potuto salvare il file temporaneo '%s' per il simulatore (disco "
"pieno?)"

# err.sim.syntx
#: it/messages:488
msgid "Syntax error in '%s' command."
msgstr "Errore di sintassi nel comando '%s'."

# err.sim.noincl
#: it/messages:489
