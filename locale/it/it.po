msgstr "Non posso eseguire il comando di stampa '%s'."

# err.badpsopen
#: it/messages:479
msgid "Unable to open file '%s' for postscript output."
msgstr "Non posso aprire il '%s' per output in postscript."

# err.badid
#: it/messages:480
