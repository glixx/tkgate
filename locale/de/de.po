msgstr "Bibliotheksmodul"

# ho.mod.type.unknown
#: de/messages:2197
msgid "Unknown module type"
msgstr "Unbekannter Modultyp"

# ho.mod.type.unused
#: de/messages:2198
msgid "Set of unused modules"
msgstr "Sammlung unbenutzer Module"

# ho.mod.type.conflict
#: de/messages:2199
msgid "Module recursion"
msgstr "Modulrekursion"

# ho.mod.type.root
#: de/messages:2200
msgid "Top-level module"
msgstr "Oberstes (Toplevel-) Modul"

# ho.mod.type.netlist
#: de/messages:2201
msgid "Netlist module"
msgstr "Netzlisten Modul"

# ho.mod.type.Lnetlist
#: de/messages:2202
msgid "Netlist module (locked)"
msgstr "Netzlisten Modul (locked)"

# ho.mod.type.hdl
#: de/messages:2203
