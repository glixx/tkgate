msgid ""
"<b>Simulator Script Example:</b>\n"
"<font face=courier>\n"
"initial\n"
"  begin\n"
"    $display(\"%t: Start of script\",$time);\n"
"    $tkg$probe(ck, reg_out, clr);\n"
"    $display(\"%t: Set probes\",$time);\n"
"    clr = 1'b0;\n"
"    #200;\n"
"    clr = 1'b1;\n"
"    $display(\"%t: Done with reset\",$time);\n"
"    #500;\n"
"    $stop();\n"
"  end\n"
"</font>"
msgstr ""
"<b>Simulator Skript Beispiel:</b>\n"
"<font face=courier>\n"
"initial\n"
"  begin\n"
"    $display(\"%t: Start des Skripts\",$time);\n"
"    $tkg$probe(ck, reg_out, clr);\n"
"    $display(\"%t: Tastkoepfe setzen\",$time);\n"
"    clr = 1'b0;\n"
"    #200;\n"
"    clr = 1'b1;\n"
"    $display(\"%t: Ende mit Reset\",$time);\n"
"    #500;\n"
"    $stop();\n"
"  end\n"
"</font>"

